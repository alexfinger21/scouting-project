                    <% } %>
                </tr>
                <!--NAME-->
                <tr>
                    <td>
                        Name
                    </td>
                    <% for(let i = 3; i < 6; i++) { %>
                        <td >
                            <%= matchup[i].tm_name %>
                        </th>
                    <% } %>
                </tr>
                <!--RANK-->
                <tr>
                    <td>
                        Rank
                    </td>
                    <% for(let i = 3; i < 6; i++) { %>
                        <td >
                            <%=matchup[i].api_rank%>
                        </td>
                    <% } %>
                </tr>
                <!--Win/Loss/Tie-->
                <tr>
                    <td>
                        Win/Loss/Tie
                    </td>
                    <% for(let i = 3; i < 6; i++) { %>
                        <td >
                            <%=(matchup[i].api_win || 0) + "/" + (matchup[i].api_loss || 0) + "/" + (matchup[i].api_tie || 0)%>
                        </td>
                    <% } %>
                </tr>
                <!--GAMES PLAYED-->
                <tr>
                    <td>
                        Games
                    </td>
                    <% for(let i = 3; i < 6; i++) { %>
                        <td >
                            <%=matchup[i].nbr_games || "N/A"%>
                        </td>
                    <% } %>
                </tr>
                <!--TELEOP AVG PTS-->
                <tr>
                    <td>
                        Teleop Pts Avg
                    </td>
                    <% for(let i = 3; i < 6; i++) { %>
                        <td >
                            <%=matchup[i].teleop_total_score_avg ? matchup[i].teleop_total_score_avg.toFixed(1) : "N/A"%>
                        </td>
                    <% } %>
                </tr>
                <!--AUTON AVG PTS-->
                <tr>
                    <td>
                        Auton Pts Avg
                    </td>
                    <% for(let i = 3; i < 6; i++) { %>
                        <td >
                            <%=matchup[i].auton_total_score_avg ? matchup[i].auton_total_score_avg.toFixed(1) : "N/A"%>
                        </td>
                    <% } %>
                </tr>
                <!--ENDGAME AVG PTS-->
                <tr>
                    <td>
                        Endgame Pts Avg
                    </td>
                    <% for(let i = 3; i < 6; i++) { %>
                        <td class= <%= matchup[i].team_master_tm_number%>>
                        </td>
                    <% } %>
                </tr>
                <!--OPR-->
                <tr>
                    <td>
                        OPR (Rank)
                    </td>
                    <% for(let i = 3; i < 6; i++) { %>
                        <td >
                            <%= Math.round(matchup[i].api_opr) + " (" + matchup[i].api_opr_rank + ")" %>
                        </td>
                    <% } %>
                </tr>
                <!--DPR-->
                <tr>
                    <td>
                        DPR (Rank)
                    </td>
                    <% for(let i = 3; i < 6; i++) { %>
                        <td >
                            <%= Math.round(matchup[i].api_dpr) + " (" + matchup[i].api_dpr_rank + ")" %>
                        </td>
                    <% } %>
                </tr>
                <!--AVG SCORE-->
                <tr>
                    <td>
                        Avg Score
                    </td>
                    <% for(let i = 3; i < 6; i++) { %>
                        <td >
                            <%= Math.round(matchup[i].total_game_score_avg) %>
                        </td>
                    <% } %>
                </tr>
            </table>
        </div>
        <canvas id="strategy-chart" display="false" ></canvas>
        
