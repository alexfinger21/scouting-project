<!DOCTYPE html>
<html lang="en">

<body>
    <script type="module" src="../static/scripts/bottomBar.js" defer></script>
    <div id="page-holder">
        <div class="small-input-container" id="select-match">
            <label id="select-match" for="select-match"><b>Select Match:</b></label>
            <select id="available-matches" name="select-match">
                <% for(let i = 0; i < gameNumbers.length; i++) { %>
                    <%if(gameNumbers[i].gm_number == match) {%>
                        <option selected>
                            <%= gameNumbers[i].gm_number %>
                        </option>
                    <%} else {%>
                        <option>
                            <%= gameNumbers[i].gm_number %>
                        </option>
                    <%}%>
                <% } %>
            </select>
        </div>
        
        <table id="scoring">
            <colgroup>
                <col span="1" style="width:60%">
            </colgroup>
            <tr>
                <td>Predicted Score:</td>
                <td class="R">
                    <%= Math.round(matchup[0].avg_gm_score + matchup[1].avg_gm_score + matchup[2].avg_gm_score) %>
                </td>
                <td class="B">
                    <%= Math.round(matchup[3].avg_gm_score + matchup[4].avg_gm_score + matchup[5].avg_gm_score) %>
                </td>
            </tr>
        </table>

        <div id="match-strategy-container">
            <!--RED TEAM TABLE-->
            <table class="stats">
                <colgroup>
                    <col span="1" style="width:33%">
                <!--HEADINGS-->
                <tr>
                    <th></th>
                    <% for(let i = 0; i < 3; i++) { %>
                        <th <%- `class="${matchup[i].gm_alliance} team${matchup[i].team_master_tm_number}"` %> >
                            <%= matchup[i].team_master_tm_number %>
                        </th>
                    <% } %>
                </tr>
                <!--NAME-->
                <tr>
                    <td>
                        Name
                    </td>
                    <% for(let i = 0; i < 3; i++) { %>
                        <td >
                            <%= matchup[i].tm_name %>
                        </th>
                    <% } %>
                </tr>
                <!--RANK-->
                <tr>
                    <td>
                        Rank
                    </td>
                    <% for(let i = 0; i < 3; i++) { %>
                        <td >
                            <%=matchup[i].api_rank%>
                        </td>
                    <% } %>
                </tr>
                <!--Win/Loss/Tie-->
                <tr>
                    <td>
                        Win/Loss/Tie
                    </td>
                    <% for(let i = 0; i < 3; i++) { %>
                        <td >
                            <%=(matchup[i].api_win || 0) + "/" + (matchup[i].api_loss || 0) + "/" + (matchup[i].api_tie || 0)%>
                        </td>
                    <% } %>
                </tr>
                <!--GAMES PLAYED-->
                <tr>
                    <td>
                        Games
                    </td>
                    <% for(let i = 0; i < 3; i++) { %>
                        <td >
                            <%=matchup[i].games_played || "N/A"%>
                        </td>
                    <% } %>
                </tr>
                <!--AUTON CHARGE STATION-->
                <tr>
                    <td>
                        Avg Auton<br>Chg Station
                    </td>
                    <% for(let i = 0; i < 3; i++) { %>
                        <td >
                            <%= Math.round(matchup[i].avg_auton_chg_station_score) || "N/A"%>
                        </td>
                    <% } %>
                </tr>
                <!--AVG LINKS-->
                <tr>
                    <td>
                        Avg Links
                    </td>
                    <% for(let i = 0; i < 3; i++) { %>
                        <td >
                            <% if(matchup[i].avg_nbr_links) {%> 
                                <%=matchup[i].avg_nbr_links.toFixed(1)%>
                            <%} else { %>
                                N/A
                            <% } %>
                        </td>
                    <% } %>
                </tr>
                <!--ENDGAME CHARGE STATION-->
                <tr>
                    <td>
                        Avg Endgame<br>Chg Station
                    </td>
                    <% for(let i = 0; i < 3; i++) { %>
                        <td class= <%= matchup[i].team_master_tm_number%>>
                            <%=Math.round(matchup[i].avg_endgame_chg_station_score) || "N/A"%>
                        </td>
                    <% } %>
                </tr>
                <!--OPR-->
                <tr>
                    <td>
                        OPR (Rank)
                    </td>
                    <% for(let i = 0; i < 3; i++) { %>
                        <td >
                            <%= Math.round(matchup[i].api_opr) + " (" + matchup[i].api_opr_rank + ")" %>
                        </td>
                    <% } %>
                </tr>
                <!--DPR-->
                <tr>
                    <td>
                        DPR (Rank)
                    </td>
                    <% for(let i = 0; i < 3; i++) { %>
                        <td >
                            <%= Math.round(matchup[i].api_dpr) + " (" + matchup[i].api_dpr_rank + ")" %>
                        </td>
                    <% } %>
                </tr>
                <!--AVG SCORE-->
                <tr>
                    <td>
                        Avg Score
                    </td>
                    <% for(let i = 0; i < 3; i++) { %>
                        <td >
                            <%= Math.round(matchup[i].avg_gm_score) %>
                        </td>
                    <% } %>
                </tr>
            </table>

            <!-------------------------------------------------------------------------------------->

            <!--BLUE TEAM TABLE-->
